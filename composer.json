{
    "name": "ursuleacv/kangaroo-rewards-api",
    "description": "Kangaroo Rewards API",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Ursuleac",
            "email": "ursuleacv@gmail.com",
            "homepage": "http://valnet.ca",
            "role": "Developer"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "api",
        "Kangaroo Rewards",
        "Kangaroo",
        "loyalty"
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "KangarooRewards\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KangarooRewards\\Api\\Test\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
